type Asset = record {
  owner : text;
  data : blob;
  content_type : text;
  chunks : vec blob;
};
service : {
  add_asset_chunk : (nat64, blob) -> ();
  get_asset : (nat64) -> (opt Asset) query;
  get_asset_ids : () -> (vec nat64) query;
  get_owner : (nat64) -> (opt text) query;
  store_asset_chunk : (blob, opt nat64) -> (nat64);
}
