type Nft = record {
  status : NftStatus;
  token_id : nat64;
  owner : principal;
  time : nat64;
  price : nat64;
};
type NftStatus = variant { Listed; Unlisted; Reimbursed };
type Result = variant { Ok : text; Err : text };
service : {
  buy_nft : (nat64) -> (Result);
  cancel_nft_listing : (nat64) -> (Result);
  get_listing : () -> (vec record { nat64; Nft }) query;
  list_nft : (nat64, nat64) -> (Result);
  update_nft_price : (nat64, nat64) -> (Result);
}
